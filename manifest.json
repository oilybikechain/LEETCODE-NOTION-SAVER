{
    "manifest_version": 2,
    "name": "LeetNotion Sync",
    "version": "1.0",
    "description": "Save completed LeetCode problems to Notion.",
    "permissions": ["activeTab", "storage", "webRequest"],
    "icons": {
      "48": "icons/icon.png"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_icon": "icons/icon.png",
      "default_popup": "popup.html"
    }
  }
  